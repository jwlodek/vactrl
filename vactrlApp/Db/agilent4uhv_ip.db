## Database for Varian Dual Ion pump
## Mar 23, 2011, BNL

## Substitutions
## Sys		-	System
## Dev		-	Device name
## Dev		-	Pump controller name
## CHAN		- 	Ion pump channel
## PORT		-	IP or Serial port name
## TCG		- 	TCG software interlock

# Get voltage measurement
record(longin, "$(Sys)$(Dev)E-I")
{
        field(DESC, "Voltage Measurement")
        field(DTYP, "stream")
        field(INP,  "@agilent4uhv.proto readNumCmd(810) $(PORT)")
        field(EGU,  "V")
        field(HOPR, "7000")
        field(LOPR, "0")
	field(SCAN, "1 second")
	field(FLNK, "$(Sys)$(Dev)I-I")
}

# Get current measurement
record(ai, "$(Sys)$(Dev)I-I")
{
        field(DESC, "Current Measurement")
        field(DTYP, "stream")
        field(INP,  "@agilent4uhv.proto readAlphaExpCmd(811) $(PORT)")
        field(EGU,  "A")
	field(PREC, "1")
	field(FLNK, "$(Sys)$(Dev)P-I")
}

# Get pressure measurement
record(ai, "$(Sys)$(Dev)P-I")
{
        field(DESC, "Pressure Measurement")
        field(DTYP, "stream")
        field(INP,  "@agilent4uhv.proto readAlphaExpCmd(812) $(PORT)")
        field(EGU,  "Torr")
        field(PREC, "1")
	field(HOPR, "1.0E-3")
	field(LOPR, "1.0E-11")	
	field(HIHI, "2.0E-7")
	field(HIGH, "4.0E-8")
	field(LOLO, "0")
	field(HHSV, "MAJOR")
	field(HSV,  "MINOR")
	field(LLSV, "MINOR")
	field(FLNK, "$(Sys)$(Dev)DB:SV-Calc_")
}

# Get severity status
record(calcout, "$(Sys)$(Dev)DB:SV-Calc_")
{
        field(DESC, "Sts Calc")
        field(INPA, "$(Sys)$(Dev)Sts:HV-Sts MSS PP")
        field(INPB, "$(Sys)$(Dev)P-I NMS NPP")
        field(FLNK, "$(Sys)$(Dev)DB:Scan-Calc_")
}

# Calc the sequence for scan
record(calcout, "$(Sys)$(Dev)DB:Scan-Calc_")
{
        field(DESC, "Scan Calc")
        field(CALC, "(A<B)?(A+C):D")
        field(INPA, "$(Sys)$(Dev)DB:Scan-Calc_.VAL NPP")
        field(INPB, "6")
        field(INPC, "1")
        field(INPD, "1")
        field(OUT,  "$(Sys)$(Dev)DB:Scan-FOut_.SELN")
        field(FLNK, "$(Sys)$(Dev)DB:Scan-FOut_")
}

record(fanout, "$(Sys)$(Dev)DB:Scan-FOut_")
{
        field(DESC, "Scan Fanout")
        field(SELM, "Specified")
        field(LNK1, "$(Sys)$(Dev)Type:Dev-Sts")
        field(LNK2, "$(Sys)$(Dev)Pwr:Max-RB")
	field(LNK3, "$(Sys)$(Dev)E:Max-RB")
        field(LNK4, "$(Sys)$(Dev)I:Prot-RB")
        field(LNK5, "$(Sys)$(Dev)P:SP-RB")
	field(LNK6, "$(Sys)$(Dev)Mode:StartProt-Sts")
}

# HV on/off
record(bi, "$(Sys)$(Dev)Sts:HV-Sts")
{
	field(DESC, "HV On/Off")
	field(DTYP, "stream")       
	field(INP,  "@agilent4uhv.proto readLogicCmd(011) $(PORT)")
	field(ZNAM, "HV Off")
	field(ONAM, "HV On")
	field(ZSV,  "MINOR")
}

record(bo, "$(Sys)$(Dev)Cmd:HV-Cmd")
{
        field(DESC, "Set HV On/Off")
        field(DTYP, "stream")
        field(OUT,  "@agilent4uhv.proto writeLogicCmd(011) $(PORT)")
	field(FLNK, "$(Sys)$(Dev)Sts:HV-Sts")
        field(ZNAM, "HV Off")
        field(ONAM, "HV On")
        field(ZSV,  "MINOR")
	info(autosaveFields_pass0, "VAL")
}

