record(calc, "$(Sys){}P:GV1GV2Avg-I")
{
        field(DESC, "P:GV1GV2")
        field(INPA, "$(Sys){}P:GV1RF_Avg-I MSS PP")
        field(INPB, "$(Sys){}P:RF_Avg-I MSS PP")
        field(INPC, "$(Sys){}P:RF_GV2Avg-I MSS PP")
	field(CALC, "(A*2.6+B*2.3+C*3.8)/8.7")
        field(PREC, "1")
        field(HIGH, "4.0E-8")
        field(HIHI, "2.0E-7")
        field(LOLO, "1.0E-12")
        field(HSV,  "MINOR")
        field(HHSV, "MAJOR")
        field(LLSV, "MINOR")
}

