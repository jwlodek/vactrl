# Average pressure 
record(sub, "$(Sys){}P:GV1RF_Avg-I")
{
	field(DESC, "P:GV1")
	field(INPA, "$(Sys):G1{CCG:1}P-I MSS NPP")
	field(INPB, "$(Sys):G1{IP:1}P-I MSS NPP")
	field(INPC, "$(Sys):G1{IP:2}P-I MSS NPP")
	field(INAM, "subAvgPrInit")
	field(SNAM, "subCommonAvgPrCalc")
	field(PREC, "1")
        field(HIHI, "2.0E-7")
        field(HIGH, "4.0E-8")
        field(LOLO, "1.0E-12")
        field(HHSV, "MAJOR")
        field(HSV,  "MINOR")
	field(LLSV, "MINOR")
}

