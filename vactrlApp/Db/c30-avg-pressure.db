# Average pressure calc

# G1 average pressure
record(sub, "$(Sys){}P:GV1GV2Avg-I")
{
        field(DESC, "P:GV1GV2")
        field(INPA, "$(Sys):G1{CCG:1}P-I MSS NPP")
        field(INPB, "$(Sys):G1{CCG:2}P-I MSS NPP")
        field(INPC, "$(Sys):G1{IP:1}P-I MSS NPP")
        field(INPD, "$(Sys):G1{IP:2}P-I MSS NPP")
        field(INPE, "$(Sys):G1{IP:3}P-I MSS NPP")
        field(INAM, "subAvgPrInit")
        field(SNAM, "subCommonAvgPrCalc")
        field(PREC, "1")
        field(HIHI, "2.0E-7")
        field(HIGH, "4.0E-8")
        field(LOLO, "1.0E-12")
        field(HHSV, "MAJOR")
        field(HSV,  "MINOR")
        field(LLSV, "MINOR")
}

record(sub, "$(Sys){}P:GV2Avg-I")
{
        field(DESC, "GV2 Avg P")
        field(INPA, "$(Sys):G3{CCG:1}P-I MSS NPP")
        field(INPB, "$(Sys):G3{IP:1}P-I MSS NPP")
	field(INPC, "$(Sys):G4{IP:1}P-I MSS NPP")
	field(INPD, "$(Sys):G4{IP:2}P-I MSS NPP")
	field(INPE, "$(Sys):G5{CCG:1}P-I MSS NPP")
	field(INPF, "$(Sys):G5{IP:1}P-I MSS NPP")
	field(INPG, "$(Sys):G6{IP:1}P-I MSS NPP")
	field(INAM, "subAvgPrInit")
        field(SNAM, "subCommonAvgPrCalc")	
	field(PREC, "1")
	field(HIGH, "4.0E-8")
        field(HIHI, "2.0E-7")
	field(LOLO, "1.0E-12")
        field(HSV,  "MINOR")
        field(HHSV, "MAJOR")
	field(LLSV, "MINOR")
}

record(sub, "$(Sys){}P:Avg-I")
{
        field(DESC, "Cell Avg P")
        field(INPA, "$(Sys){}P:GV1GV2Avg-I MSS CPP")
        field(INPB, "$(Sys){}P:GV2Avg-I MSS PP")
        field(INAM, "subAvgPrInit")
        field(SNAM, "subEvenCellAvgPrCalc")
        field(SCAN, "1 second")
        field(PREC, "1")
        field(HIGH, "4.0E-8")
        field(HIHI, "2.0E-7")
        field(LOLO, "1.0E-12")
        field(HSV,  "MINOR")
        field(HHSV, "MAJOR")
        field(LLSV, "MINOR")
}

