## Database for SR Straight TSP Parameter download
## Mar 5, 2014, BNL

record(mbbo, "$(Sys):G1{}DB:TSP_ActvFil-Sel")
{
	field(DESC, "Filament Sel")
	field(ZRST, "Mini Ti-Ball")
	field(ONST, "Filament 1")
	field(TWST, "Filament 2")
	field(THST, "Filament 3")
	field(VAL,  "1")
	field(ASG,  "VAWRITE")
	info(autosaveFields_pass0, "VAL")
}

record(ao, "$(Sys):G1{}DB:TSP_SublI-SP")
{
        field(DESC, "Sublimation I")
        field(ASG,  "VAWRITE")
	field(HOPR, "50")
	field(LOPR, "30")
	field(LINR, "SLOPE")
	field(ESLO, "0.1")	
	field(EGU,  "A")
	field(PREC, "1")
	field(VAL,  "48")
	info(autosaveFields_pass0, "VAL")
}

