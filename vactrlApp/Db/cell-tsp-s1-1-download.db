record(bo, "$(Sys):G1{}DB:TSP_Dwnld-Cmd")
{
        field(DESC, "Download TSP Setting")
        field(ASG,  "VAWRITE")
        field(VAL,  "1")
	field(FLNK, "$(Sys):G1{}DB:TSP_Dwnld-Seq_")
}

record(seq, "$(Sys):G1{}DB:TSP_Dwnld-Seq_")
{
	field(DESC, "Straight TSP Dwnld Seq")
	field(SELM, "All")
	field(DO1, "1")
	field(LNK1, "$(Sys):G1{TSP:1}DB:Dwnld-Cmd PP")
}

record(bo, "$(Sys):G1{}DB:TSP_StartStop-Cmd")
{
	field(DESC, "Start/Stop Sublimation")
	field(ASG,  "VAWRITE")
	info(autosaveFields_pass0, "VAL")
	field(ZNAM, "Stop")
	field(ONAM, "Start")
	field(FLNK, "$(Sys):G1{}DB:TSP_StartStop-DFOut_")
}

record(dfanout, "$(Sys):G1{}DB:TSP_StartStop-DFOut_")
{
        field(DESC, "Straight TSP Start Sublimation")
	field(OMSL, "closed_loop")
	field(SELM, "All")
	field(DOL,  "$(Sys):G1{}DB:TSP_StartStop-Cmd")
        field(OUTA, "$(Sys):G1{TSP:1}Mode:StartStop-Cmd PP")
}

