# Calc the sequence for scan
record(calcout, "$(Sys)$(Dev)DB:Scan-Calc_") 
{
	field(DESC, "Scan Calc")
	field(CALC, "(A<B)?(A+C):D")
	field(INPA, "$(Sys)$(Dev)DB:Scan-Calc_")
	field(INPB, "7")
	field(INPC, "1")
	field(INPD, "1")
	field(OUT,  "$(Sys)$(Dev)DB:Scan2-FOut_.SELN")
	field(FLNK, "$(Sys)$(Dev)DB:Scan2-FOut_")
}

# Fanout record
record(fanout, "$(Sys)$(Dev)DB:Scan2-FOut_")
{
	field(DESC, "Scan Fanout")
	field(SELM, "Specified")
	field(LNK1, "$(Sys)$(Dev)Sts:AutoStart-Sts")
	field(LNK2, "$(Sys)$(Dev)Sts:ActvFil-Sts")
	field(LNK3, "$(Sys)$(Dev)Mode:Opr-Sts")
	field(LNK4, "$(Sys)$(Dev)I:Subl-RB")
	field(LNK5, "$(Sys)$(Dev)Time:SublPerd-Sts")
	field(LNK6, "$(Sys)$(Dev)Time:Subl-RB")
}

