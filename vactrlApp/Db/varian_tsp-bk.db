# Dev status
record(mbbi, "$(Sys)$(Dev)Sts:Ctrl-Sts")
{
	field(DESC, "Get Status")
	field(DTYP, "stream")       
	field(INP,  "@varian_tsp.proto getCtrlStatus $(PORT)")
	field(ZRST, "Stop")
	field(ONST, "Fail")
	field(TWST, "Wait Interlock")
	field(THST, "Ramp")
	field(FRST, "Wait Subl")
	field(FVST, "Sublimation")
	field(UNSV, "INVALID")
	field(ZRSV, "NO_ALARM")
	field(ONSV, "MINOR")
  	field(TWSV, "NO_ALARM")
	field(THSV, "NO_ALARM")
	field(FRSV, "NO_ALARM")
	field(FVST, "NO_ALARM")	
